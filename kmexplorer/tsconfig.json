{
	"compilerOptions": {
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"noImplicitThis": true,
		"baseUrl": "./",
		"paths": {
			"~/*": [
				"./app/*"
			],
			"@/*": [
				"./*"
			],
			"%/*": [
				"../kmserver-core/src/*"
			]
		},
		"experimentalDecorators": true
	},
	"references": [
		 { "path": "./.nuxt/tsconfig.app.json" },
		{ "path": "./.nuxt/tsconfig.server.json" },
		{ "path": "./.nuxt/tsconfig.shared.json" },
		{ "path": "./.nuxt/tsconfig.node.json" }
  ]
}
