{
	"extends": "./.nuxt/tsconfig.app.json",
	"compilerOptions": {
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"noImplicitThis": true,
		"baseUrl": "./",
		"paths": {
			"~/*": [
				"./app/*"
			],
			"@/*": [
				"./*"
			],
			"%/*": [
				"../kmserver-core/src/*"
			]
		},
		"experimentalDecorators": true
	},
}
